<% if(success && success.length){%>
    <div class="alert col-md-4 offset-md-4 alert-success alert-dismissible fade show" role="alert">
        <%=success%>
            <button type="button" class="btn-close " data-bs-dismiss="alert" aria-label="Close">
              
            </button>

    </div>
    <% }%>

    <% if(error && error.length){%>
    <div class="alert col-md-4 offset-md-4 alert-danger alert-dismissible fade show" role="alert">
        <%=error%>
            <button type="button" class="btn-close " data-bs-dismiss="alert" aria-label="Close">
              
            </button>

    </div>
    <% }%>